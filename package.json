    },
    "devDependencies": {
        "@types/node": "^20.14.11",
        "@vscode/vsce": "^2.30.0",
        "chalk": "^5.3.0",
        "colorjs.io": "^0.5.2",
        "prettier": "^3.3.3",
        "typescript": "^5.5.3"
    }
}
